{
  "all": true,
  "include": [
    "out/src/**/*.js"
  ],
  "exclude": [
    "out/test/**",
    "out/src/test/**",
    "**/*.d.ts",
    "node_modules/**",
    "out/extension.js"
  ],
  "reporter": [
    "lcov",
    "text",
    "html",
    "json"
  ],
  "check-coverage": true,
  "lines": 70,
  "functions": 70,
  "branches": 65,
  "statements": 70,
  "report-dir": "./coverage",
  "temp-directory": "./.nyc_output"
}
